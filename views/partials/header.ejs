    <!-- Favicon  -->
    <link rel="shortcut icon" href="/img/favicon.webp" type="image/png" />

    <!-- CSS  -->
    <link rel="stylesheet" href="/style.css" />

    <!-- title  -->
    <title><%= heading %></title>
  </head>
  <body>
